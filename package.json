{
  "name": "pollster",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "Poll tracking application",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/pollster"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "4.11.6",
    "body-parser": "1.18.2",
    "cookie-session": "1.3.2",
    "express": "4.16.2",
    "helmet": "3.9.0",
    "mongodb": "2.2.33",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "prop-types": "15.6.0",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "0.11.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "test": "npm run test-db && npm run test-server && npm run test-store",
    "test-server": "mocha test/server/test-main.js",
    "test-db": "mocha test/db/test-main.js",
    "test-store": "mocha --compilers js:babel-register test/store/test-main.js",
    "start": "nodemon dist/main.js --port=3000",
    "build": "gulp",
    "build-stage": "gulp stage",
    "lint": "eslint --ext .js --ext .jsx src/** && eslint test/**",
    "lintrules": "eslint eslint-rules/**",
    "coverage": "npm run c-db && npm run c-server && npm run c-combine",
    "c-server": "./node_modules/istanbul/lib/cli.js cover --dir ./coverage/server ./node_modules/mocha/bin/_mocha test/server/test-main.js -- -R spec",
    "c-db": "./node_modules/istanbul/lib/cli.js cover --dir ./coverage/db ./node_modules/mocha/bin/_mocha test/db/test-main.js -- -R spec",
    "c-combine": "./node_modules/istanbul/lib/cli.js report --dir ./coverage --include **/*coverage.json json lcov"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "chai": "4.1.2",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "7.0.1",
    "fetch-cookie": "0.7.0",
    "gulp": "3.9.1",
    "gulp-babel-minify": "0.2.0",
    "gulp-cssmin": "0.2.0",
    "gulp-gzip": "1.4.0",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-util": "3.0.8",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nock": "9.0.9",
    "node-fetch": "1.7.3",
    "nodemon": "1.11.0",
    "redux-mock-store": "1.2.3",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.9.0"
  },
  "engines": {
    "node": "8.9.0"
  }
}
