{
  "name": "pollster",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "Poll tracking application",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/pollster"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "4.11.3",
    "babel-polyfill": "6.23.0",
    "body-parser": "1.16.1",
    "cookie-session": "1.2.0",
    "express": "4.14.1",
    "helmet": "3.4.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "mongodb": "2.2.24",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "test": "npm run test-db && npm run test-server && npm run test-store",
    "test-server": "mocha test/server/test-main.js",
    "test-db": "mocha test/db/test-main.js",
    "test-store": "mocha --compilers js:babel-register test/store/test-main.js",
    "start": "nodemon dist/main.js --port=3000",
    "build": "gulp",
    "build-stage": "gulp stage",
    "lint": "eslint --ext .js --ext .jsx src/** && eslint test/**",
    "lintrules": "eslint eslint-rules/**",
    "coverage": "npm run c-db && npm run c-server && npm run c-combine",
    "c-server": "./node_modules/istanbul/lib/cli.js cover --dir ./coverage/server ./node_modules/mocha/bin/_mocha test/server/test-main.js -- -R spec",
    "c-db": "./node_modules/istanbul/lib/cli.js cover --dir ./coverage/db ./node_modules/mocha/bin/_mocha test/db/test-main.js -- -R spec",
    "c-combine": "./node_modules/istanbul/lib/cli.js report --dir ./coverage --include **/*coverage.json json lcov"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babelify": "7.3.0",
    "browserify": "14.1.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "gulp": "3.9.1",
    "gulp-cssmin": "0.1.7",
    "gulp-gzip": "1.4.0",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.4.1",
    "gulp-uglify": "2.0.1",
    "gulp-util": "3.0.8",
    "isomorphic-fetch": "2.2.1",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nock": "9.0.9",
    "nodemon": "1.11.0",
    "react-addons-test-utils": "15.4.2",
    "redux-mock-store": "1.2.2",
    "request": "2.79.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.9.0"
  },
  "engines": {
    "node": "6.9.5"
  }
}
