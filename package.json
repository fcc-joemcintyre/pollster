{
  "name": "pollster",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "Poll tracking application",
  "type": "module",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/pollster"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "6.12.4",
    "cookie-session": "1.4.0",
    "express": "4.17.1",
    "helmet": "3.23.3",
    "mongodb": "3.6.0",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "react-redux": "7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "styled-components": "5.1.1"
  },
  "scripts": {
    "start": "nodemon --watch dist dist/main.js -p=3000",
    "build": "webpack --config=webpack.config.cjs --env=development -w",
    "build-stage": "webpack --config=webpack.config.cjs --env=production",
    "test": "jest",
    "test-i": "mocha test/db",
    "test-int": "mocha --require @babel/register test/server/test-main.js test/db/test-main.js test/store/test-main.js",
    "lint": "eslint --ext .js --ext .jsx src/** && eslint test/**",
    "coverage": "jest --coverage",
    "coverage-int": "cross-env NODE_ENV=test2 nyc mocha test/server/test-main.js test/db/test-main.js test/store/test-main.js"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/register": "7.10.5",
    "@cygns/eslint-config": "5.0.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "chai": "4.2.0",
    "compression-webpack-plugin": "5.0.1",
    "copy-webpack-plugin": "6.0.3",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "eslint": "7.7.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "fetch-cookie": "0.10.1",
    "jest": "26.4.2",
    "jest-styled-components": "7.0.3",
    "mocha": "8.1.1",
    "nock": "13.0.4",
    "node-fetch": "2.6.0",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "1.5.4",
    "webpack": "5.0.0-beta.28",
    "webpack-cli": "3.3.12"
  },
  "engines": {
    "node": "^14.8",
    "yarn": "^1.22"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/public/js/*"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jestSetup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage2",
    "coverageReporters": [
      "html"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
