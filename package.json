{
  "name": "pollster",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "Poll tracking application",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/pollster"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/register": "7.0.0",
    "ajv": "6.5.1",
    "body-parser": "1.18.3",
    "cookie-session": "1.3.2",
    "express": "4.16.3",
    "helmet": "3.12.1",
    "mongodb": "3.1.6",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "3.3.3",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "start": "nodemon --watch dist dist/main.js -p=3000",
    "build": "webpack --config webpack/webpack.dev.js -w",
    "build-stage": "webpack --config webpack/webpack.stage.js",
    "test": "jest",
    "test-integration": "npm run test-db && npm run test-server && npm run test-store && npm run test-form",
    "test-server": "mocha test/server/test-main.js",
    "test-db": "mocha test/db/test-main.js",
    "test-store": "mocha --compilers js:@babel/register test/store/test-main.js",
    "test-form": "mocha --compilers js:@babel/register test/form/test-form.js",
    "lint": "eslint --ext .js --ext .jsx src/** && eslint test/**",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@cygns/eslint-config": "3.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-styled-components": "1.5.1",
    "chai": "4.1.2",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.5.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-react": "7.9.1",
    "fetch-cookie": "0.7.0",
    "jest": "23.6.0",
    "jest-styled-components": "5.0.1",
    "mocha": "5.2.0",
    "nock": "9.3.3",
    "node-fetch": "2.1.2",
    "nodemon": "1.17.5",
    "react-test-renderer": "16.4.1",
    "redux-mock-store": "1.5.3",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0"
  },
  "engines": {
    "node": "8.11.3",
    "yarn": "1.7.0"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/public/js/*"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jestSetup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html"
    ]
  }
}
