{
  "name": "pollster",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "Poll tracking application",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/pollster"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "6.0.1",
    "body-parser": "1.18.2",
    "cookie-session": "1.3.2",
    "express": "4.16.2",
    "helmet": "3.9.0",
    "mongodb": "2.2.33",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-mock-store": "1.2.3",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "0.11.0",
    "styled-components": "3.1.6",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node dist/main.js --port=3000",
    "start:dev": "nodemon dist/main.js --port=3000",
    "test": "jest",
    "integration-test": "npm run test-db && npm run test-server && npm run test-store && npm run test-form",
    "test-server": "mocha test/server/test-main.js",
    "test-db": "mocha test/db/test-main.js",
    "test-store": "mocha --compilers js:babel-register test/store/test-main.js",
    "test-form": "mocha --compilers js:babel-register test/form/test-form.js",
    "build": "gulp",
    "build-stage": "gulp stage",
    "lint": "eslint --ext .js --ext .jsx src/** && eslint test/**",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@cygns/eslint-config": "2.1.0",
    "babel-core": "6.26.0",
    "babel-plugin-styled-components": "1.5.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "chai": "4.1.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-react": "7.7.0",
    "fetch-cookie": "0.7.0",
    "gulp": "3.9.1",
    "gulp-babel-minify": "0.2.0",
    "gulp-gzip": "1.4.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-util": "3.0.8",
    "jest": "22.4.2",
    "jest-styled-components": "5.0.0",
    "mocha": "5.0.1",
    "nock": "9.0.9",
    "node-fetch": "1.7.3",
    "nodemon": "1.11.0",
    "react-test-renderer": "16.2.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.9.0"
  },
  "engines": {
    "node": "8.9.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jestSetup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html"
    ]
  }
}
