{
  "name": "pollster",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "Poll tracking application",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/pollster"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "6.10.2",
    "cookie-session": "1.3.3",
    "express": "4.17.1",
    "helmet": "3.21.1",
    "mongodb": "3.3.2",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-redux": "7.1.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.0"
  },
  "scripts": {
    "start": "nodemon --watch dist dist/main.js -p=3000",
    "build": "webpack --env=development -w",
    "build-stage": "webpack --env=production",
    "test": "jest",
    "test-int": "mocha --require @babel/register test/server/test-main.js test/db/test-main.js test/store/test-main.js",
    "lint": "eslint --ext .js --ext .jsx src/** && eslint test/**",
    "coverage": "jest --coverage",
    "coverage-int": "cross-env NODE_ENV=test2 nyc mocha test/server/test-main.js test/db/test-main.js test/store/test-main.js"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.6.2",
    "@cygns/eslint-config": "4.0.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "chai": "4.2.0",
    "compression-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.4",
    "cross-env": "5.2.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.5.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.1.2",
    "fetch-cookie": "0.7.3",
    "jest": "24.9.0",
    "jest-styled-components": "6.3.3",
    "mocha": "6.2.1",
    "nock": "10.0.6",
    "node-fetch": "2.6.0",
    "nodemon": "1.19.3",
    "nyc": "14.1.1",
    "react-test-renderer": "16.10.2",
    "redux-mock-store": "1.5.3",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9"
  },
  "engines": {
    "node": "^10.16",
    "yarn": "^1.16"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/public/js/*"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jestSetup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage2",
    "coverageReporters": [
      "html"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
